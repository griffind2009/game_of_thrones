<h1>All Characters</h1>

<ul>
  <% @characters.each do |character| %>
    <li>
        <%= character.name %>
    </li>
    <%= link_to 'Show this Character', character_path(character) %>
<%= link_to 'Edit Character', edit_character_path(character) %>
<%= link_to 'Delete Character', character_path(character), method: :delete %>
  <% end %>
</ul>
<br>
<%= link_to 'Create a New Character', new_character_path %>
